{
    "name": "bobbybouwmann/jokes",
    "description": "Jokes generator package",
    "authors": [
        {
            "name": "Bobby Bouwmann",
            "email": "bobbybouwmann@gmail.com"
        }
    ],
    "require": {
    	"php": "^7.2",
    	"illuminate/console": "^5.7",
		"illuminate/support": "^5.7"
    },
    "require-dev": {
    	"phpunit/phpunit": "^7.5",
        "orchestra/testbench" : "~3.7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bobbybouwmann\\Jokes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bobbybouwmann\\Jokes\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bobbybouwmann\\Jokes\\JokesServiceProvider"
            ]
        }
    }
}
